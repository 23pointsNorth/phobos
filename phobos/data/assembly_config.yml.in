#      - visual_: "visual_"
#    name_replacements: {}  # dict or list
#    collision_prefix: ""
#    collision_suffix: ""
#    visual_prefix: ""
#    visual_suffix: ""

  export_floatingbase: True
  export_ros_pathes: True
  export_kccd:
    join_before_convexhull: True
    remove_joints: []
    merge_additionally: []
    keep_stls: True
    dirname: "kccd"
    reduce_meshes: 0.5
    braking_model:
      linearFactor: 0
      invDeceleration: 0.02
      uncertainty: 0
      latency: 0
    simplify_swept_from: [ ]
    safety_distance: 0
    report_up_to: 0
    computation_budget: -1
    max_approximation_order: 1

  remove_joints: []

  edit_collisions:
    default:
      join: True
      shape: box
      oriented: True
      noDataPackage: True
    exclude: [ ]

  redefine_articulation:
    default:
      backup: True
      min: ["rad", -3.1415926]
      max: ["rad", 3.1415926]
      vel: 3.1415926
      eff: 400.0
      reducedDataPackage: True

