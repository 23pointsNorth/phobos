assemble:
  ros_package: False
  has_submechanisms: False
  output_mesh_format: stl
  input_meshes_are_mars_obj: True
  also_export_bobj: True
  ensure_valid_joints: False
  compare_model:
    tolerance: 0.01
    tolerance_mass: 0.01
    tolerance_rad: 0.03
    tolerance_distance: 0.003
  export_pdf: True
  model_tests:
    - process_double_check
    - load_in_pybullet
    - compare_link_masses
    - compare_link_transformations
    - topological_self_consistency
    - compare_amount_joints
    - hyrodynChecks:
      - load_in_hyrodyn
  meshes:
    stl: "meshes/stl"
    obj: "meshes/obj"
    mars_obj: "meshes/mars_obj"
    bobj: "meshes/bobj"
    iv: "meshes/iv"
xtype_model:
  robotname: "@MODEL_NAME@"
  modelname: "@MODEL_NAME@-@MODEL_VERSION@"
  model:
    modelname: "@MODEL_NAME@"
    modelversion: "@MODEL_VERSION@"

#  compare_model:
#    git: "git@git.hb.dfki.de:models-robots/rh5v2_models/abstract-smurf"
#    branch: master
#    model_in_repo: "urdf/RH5v2.urdf"
